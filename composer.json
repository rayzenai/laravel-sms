{
    "name": "rayzenai/laravel-sms",
    "description": "A Laravel package for sending SMS messages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rayzen AI",
            "email": "support@rayzenai.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rayzenai\\LaravelSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rayzenai\\LaravelSms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rayzenai\\LaravelSms\\LaravelSmsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
